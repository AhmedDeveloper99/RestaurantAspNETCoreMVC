@model IEnumerable<RestaurantAspNETCoreMVC.Models.Register>

@{
    ViewData["Title"] = "Register";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Register</h1>


<table class="table table-bordered table-hover table-dark">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.User_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User_Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User_Number)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User_Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User_Cpassword)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User_Membership)
            </th>
          @*   <th></th> *@
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.User_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User_Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User_Number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User_Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User_Cpassword)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User_Membership)
            </td>
       @*      <td>
                <a asp-action="Edit" asp-route-id="@item.User_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.User_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.User_Id">Delete</a>
            </td> *@
        </tr>
}
    </tbody>
</table>
